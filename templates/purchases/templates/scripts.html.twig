<script>
    $(document).ready(function() {
        $('.table-item').click(function(){
            window.location = $(this).attr('href');
            return false;
        });
        $('.table-item').hover(
            function() {
                $( this ).addClass( "table-item-hover" );
            }, function() {
                $( this ).removeClass("table-item-hover" );
            }
        );
    });
</script>