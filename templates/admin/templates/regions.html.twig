{% extends "admin/base.html.twig" %}

{% block content %}

<div class="row mt-3">
    <div class="col-lg-12">
        <div class="ibox">
            <div class="ibox-title">
                <h5>Регионы</h5>
            </div>
            <div class="ibox-content">
                {% for i in regions %}
                    <a href="{{ path('app_admin_edit_region_info', {'id': i.getId()}) }}">
                        <button class="btn btn-success m-1">
                            {{ loop.index }}.
                            {{ i.getName() }}
                        </button>
                    </a>
                {% endfor %}
            </div>
        </div>
    </div>
</div>



{% endblock %}
{% block scripts %}





{% endblock %}
