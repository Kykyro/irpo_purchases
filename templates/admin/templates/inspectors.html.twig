{% extends "admin/base.html.twig" %}



{% block content %}


    <div class="row d-flex justify-content-center animated fadeInRight" style="margin-top: 20px">
        <div class="col-sm-10">
            <div class="row">
                <div class="col-sm-12">

                    <div class="ibox">
                        <div class="ibox-content">
                            <div class="row col-lg-12" style="margin-left: 2px; padding-bottom: 2rem; padding-top: 2rem">
                                {{ form_start(form) }}
                                <label> Имя пользователя </label>
                                {{ form_widget(form.search) }}

                                {{ form_widget(form.submit) }}
                                {{ form_end(form) }}
                            </div>
                        </div>
                    </div>
                    <div class="row col-lg-12">
                        {% for i in pagination %}
                            {{ include('default/inspectorCard.html.twig', {'i': i}) }}

                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}

{% block scripts %}

{% endblock %}