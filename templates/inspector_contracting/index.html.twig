{% extends 'default/base.html.twig' %}

{% block content %}
    <div class="row mt-5">
        <div class="offset-lg-3 col-lg-6">
            <div class="ibox ">
                <div class="ibox-title">
                    <h2>
                        Актуальная выгрузка

                    </h2>
                    <div class="ibox-tools">

                    </div>
                </div>
                <div class="ibox-content">
                    <h3>
                        Таблица будет сформирована на основе актуальных данных
                    </h3>
                    {{ form_start(form) }}
                        <div class="form-group">
                            <label>
                                Год создания кластеров
                            </label>
                            {{ form_widget(form.year) }}
                        </div>
                    <div class="form-group">
                        <label>
                            Тип
                        </label>
                        {{ form_widget(form.type) }}
                    </div>
                        <div class="form-group">

                            {{ form_widget(form.submit) }}
                        </div>
                    {{ form_end(form) }}
                </div>
                <div class="ibox-footer">

                </div>
            </div>
        </div>
    </div>



{% endblock %}

{% block scripts %}

{% endblock %}
