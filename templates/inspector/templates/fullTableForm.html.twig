<div class="row">
    <div class="col-sm-8 offset-sm-2 ">
        <div class="wrapper wrapper-content ">

            <div class="ibox-content block-shadow">

                <div class="row">
                    <a href="{{ path('app_inspector_main') }}">
                        <- Вернутся назад
                    </a>
                </div>

                {{ form_start(form) }}
                <div class="form-group  row">
                    <label class="col-sm-2 col-form-label text-right">Год: {{ year }}</label>
                    <label class="col-sm-2 col-form-label text-right">Субъект: {{ RFsubject.name }}</label>
                </div>
                <div class="form-group  row">
                    <label class="col-sm-2 col-form-label text-right">Организация</label>
                    {{ form_widget(form.orgName) }}
                </div>
                <div class="form-group row ">
                    <div class="col-lg-12">

                        {% if  prodProc is defined%}
                        <div class="pull-left">
                            <span class=" align-bottom">Найдено закупок - {{ prodProc|length }}</span>
                        </div>
                        {% endif %}

                        <button class="col-lg-2  btn btn-info btn-lg pull-right m-1" type="submit">
                            <nav class="fa fa-search"></nav>
                            Найти
                        </button>

                    </div>
                </div>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
</div>