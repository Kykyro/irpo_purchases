<div class="row d-flex justify-content-center">
    <div class="col-sm-6">
        <div class="wrapper wrapper-content ">

            <div class="ibox-content block-shadow">

                <div class="row m-1 ">
                    <a href="{{ path('app_inspector_main') }}">
                        Вернутся назад
                    </a>
                </div>

                {{ form_start(form) }}
                <div class="form-group row m-3 ">
                    <label class="col-sm-2 col-form-label text-left"><strong>Год:</strong> {{ year }}</label>
                </div>
                <div class="form-group row m-3">
                    <label class="col-sm-4 col-form-label text-left"><strong>Субъект:</strong> {{ RFsubject.name }}</label>
                </div>
                <div class="form-group row m-3">
                    <label class="col-sm-2 col-form-label text-left"><strong>Организация:</strong></label>
                    <label class="col-sm-10">{{ form_widget(form.orgName) }}</label>
                </div>
                <div class="form-group row ">
                    <div class="col-lg-12">

                        {% if  prodProc is defined%}
                        <div class="pull-left m-1">
                            <span class=" align-bottom">Найдено закупок - {{ prodProc|length }}</span>
                        </div>
                        {% endif %}
                    </div>
                </div>
                <div>
                    <div class="col-lg-12 ">
                        <button class="col-lg-12  btn btn-outline-success btn-lg " type="submit">
                            <nav class="fa fa-search"></nav>
                            Найти
                        </button>
                    </div>
                </div>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
</div>