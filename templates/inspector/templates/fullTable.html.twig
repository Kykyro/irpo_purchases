                    </thead>
                    <tbody>
                    {% for i in prodProc %}
                        <tr>
                            <td>{{ loop.index }}</td>
                            <td>{{ i.getPurchaseObject() }}</td>
                            <td>{{ i.getMethodOfDetermining() }}</td>
                            <td>%sum%</td>
                            <td>{{ i.getinitialFederalFunds() }}</td>
                            <td>{{ i.getinitialFundsOfSubject() }}</td>
                            <td>{{ i.getinitialEmployersFunds() }}</td>
                            <td>{{ i.getinitialEducationalOrgFunds() }}</td>
                            <td>{{ i.getpublicationDate().format('d/m/Y') }}</td>
                            <td>{{ i.getdeadlineDate().format('d/m/Y') }}</td>
                            <td>{{ i.getdateOfSummingUp().format('d/m/Y') }}</td>
                            <td>{{ i.getPurchaseLink() }}</td>
                            <td>{{ i.getPurchaseNumber() }}</td>
                            <td>{{ i.getpostponementDate().format('d/m/Y') }}</td>
                            <td>{{ i.getpostonementComment() }}</td>
                            <td>{{ i.getpostonementComment() }}</td> {# дата заключения контракта #}
                            <td>{{ i.getsupplierName() }}</td>
                            <td>{{ i.getsupplierINN() }}</td>
                            <td>{{ i.getsupplierKPP() }}</td>
                            <td>%sum%</td>
                            <td>{{ i.getfinFederalFunds() }}</td>
                            <td>{{ i.getfinFundsOfSubject() }}</td>
                            <td>{{ i.getfinFundsOfEducationalOrg() }}</td>
                            <td>{{ i.getfinEmployersFunds() }}</td>
                            <td>{{ i.getDeliveryTime().format('d/m/Y') }}</td>
                            <td>{{ i.getComments() }}</td>

                        </tr>
                    {% endfor %}
